{
  "expo": {
    "name": "FlashCardFrenzy",
    "slug": "FlashCardFrenzy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jeramai.FlashCardFrenzy",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#f3ffe0"
      },
      "package": "com.jeramai.FlashCardFrenzy"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#f3ffe0"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "baseUrl": "/FlashCardFrenzy"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "da56700b-8e4d-4035-b45a-efc98cb5dfde"
      }
    }
  }
}
